{
    "name": "@kolbasik/azure-chrome-plugin",
    "version": "1.0",
    "manifest_version": 3,
    "icons": {
        "16": "/images/16.png",
        "32": "/images/32.png",
        "48": "/images/48.png",
        "128": "/images/128.png"
    },
    "content_scripts": [{
        "matches": ["https://portal.azure.com/*"],
        "js": ["script.js"]
    }]
}